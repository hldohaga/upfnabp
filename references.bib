  keywords = {Blood pressure,Cardiovascular diseases,Hypertension,Ultra-processed food,upf and bp},
  file = {/home/david/Zotero/storage/V4Z7G9DU/Scaranni et al. - 2021 - Ultra-processed foods, changes in blood pressure a.pdf;/home/david/Zotero/storage/NLXIZWJC/1A120EFBE6785C030961E19B94977D9B.html}
}

@thesis{schulzeUPFCardiometabolicHealth2019,
  title = {{{UPF}} and Cardiometabolic Health},
