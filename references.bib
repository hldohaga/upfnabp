@article{romeroferreiroTwoDimensionsNutritional2021,
  title = {Two {{Dimensions}} of {{Nutritional Value}}: {{Nutri-Score}} and {{NOVA}}},
  shorttitle = {Two {{Dimensions}} of {{Nutritional Value}}},
  author = {Romero Ferreiro, Carmen and Lora Pablos, David and Gómez de la Cámara, Agustín},
  date = {2021-08-13},
