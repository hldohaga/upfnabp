  author = {Vollmer, William M. and Sacks, Frank M. and Ard, Jamy and Appel, Lawrence J. and Bray, George A. and Simons-Morton, Denise G. and Conlin, Paul R. and Svetkey, Laura P. and Erlinger, Thomas P. and Moore, Thomas J. and Karanja, Njeri and {for the DASH-Sodium Trial Collaborative Research Group}},
