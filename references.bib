  author = {Scaranni, Patricia de Oliveira da Silva and Cardoso, Leticia de Oliveira and Chor, Dora and Melo, Enirtes Caetano Prates and Matos, Sheila Maria Alvim and Giatti, Luana and Barreto, Sandhi Maria and family=Fonseca, given=Maria de Jesus Mendes, prefix=da, useprefix=true},
  date = {2021},
  journaltitle = {Public health nutrition},
  volume = {24},
  number = {11},
  pages = {3352--3360},
  publisher = {{Cambridge University Press}},
  location = {{Cambridge, UK}},
  issn = {1368-9800},
  abstract = {Objective: To estimate changes in blood pressure and the incidence of hypertension associated with consumption of ultra-processed foods (UPF) by Brazilian civil servants at a 4-year follow-up. Design: Longitudinal analysis of the ELSA-Brasil with non-hypertensive individuals at baseline. We applied the FFQ at the baseline and categorised energy intake by degree of processing, using the NOVA classification. Systolic blood pressure (SBP) and diastolic blood pressure (DBP) were measured at baseline (2008-2010) and again at first follow-up (2012-2014). Incidence of arterial hypertension was defined as SBP ¿= 140 mmHg or DBP ¿= 90 mmHg or antihypertensive medication during the previous 2 weeks. A mixed-effect linear regression model and mixed-effect logistic regression model were used to estimate associations between UPF consumption and, respectively, changes in blood pressure and incidence of hypertension. Setting: Brazil. Participants: Civil servants of Brazilian public academic institutions in six cities (n 8754), aged 35-74 years at baseline (2008-2010). Results: UPF consumption contributed 25 center dot 2 \% (sd = 9 center dot 6) of total energies consumed. After adjustment, participants with high UPF consumption presented a 23 \% greater risk of developing hypertension (OR = 1 center dot 23, 95 \% CI 1 center dot 06, 1 center dot 44) than those with low UPF consumption. We did not find association between UPF consumption and changes in blood pressure over time. Conclusions: The higher the UPF consumption, the higher the risk of hypertension in adults. Reducing UPF consumption is thus important to promote health and prevent hypertension.},
  copyright = {The Author(s), 2021. Published by Cambridge University Press on behalf of The Nutrition Society},
  langid = {english},
  keywords = {Adults,Antihypertensives,Blood pressure,Bread,Cardiovascular diseases,Eating behavior,Energy,Energy intake,Environmental & Occupational Health,Food,Food consumption,Food processing,Gastrointestinal surgery,Health promotion,Hypertension,Life Sciences & Biomedicine,Longitudinal studies,Mortality,Nutrition & Dietetics,Nutritional epidemiology,Processed foods,Public,Questionnaires,Regression models,Research paper,Science & Technology,Ultra-processed food}
}

