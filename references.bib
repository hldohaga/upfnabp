@article{Rezende-AlvesKatiusse2021Fpar,
  title = {Food Processing and Risk of Hypertension: {{Cohort}} of Universities of Minas Gerais, Brazil ({{CUME}} Project)},
  author = {Rezende-Alves, Katiusse and Hermsdorff, Helen Hermana Miranda and Miranda, Aline Elizabeth da Silva and Lopes, Aline Cristine Souza and Bressan, Josefina and Pimenta, Adriano Marçal},
  date = {2021},
  journaltitle = {Public health nutrition},
  volume = {24},
  number = {13},
  pages = {4071--4079},
  publisher = {{Cambridge University Press}},
  location = {{Cambridge, UK}},
  issn = {1368-9800},
  abstract = {Objective: To analyse the association between food consumption according to the degree of processing and incidence of hypertension in CUME project participants. Design: Longitudinal study in which food consumption was evaluated according to the percentage contribution of daily energetic intake (\%/d) of each NOVA classification group (unprocessed/minimally processed foods and culinary preparations (U/MPF \& CP); processed foods and ultra-processed foods (UPF)). Hypertension was defined according to American College of Cardiology/American Heart Association (ACC/AHA) criteria. Adjusted relative risks (RR) and their 95\% confidence intervals (95 \% CI) were estimated by Poisson regression models with robust variances. Setting: Brazil. Participants: 1221 graduates classified as non-hypertensive at baseline and monitored for 2 years. Results: Daily energetic percentage from each group according to degree of processing was 64 center dot 3 (sd 12) \% for U/MPF \& CP; 9 center dot 9 (sd 5 center dot 8) \% for processed foods and 25 center dot 8 (sd 11) \% for UPF. Incidence of hypertension was high (152/1000 person-years; n 113, 193/1000 person-years in males and n 257, 138/1000 person-years in females). After adjusting for potential confounders, participants in the upper quintile of daily energetic intake of U/MPF\&CP presented a reduced risk of hypertension (RR: 0 center dot 72; 95 \% CI 0 center dot 52, 0 center dot 98), while those in the upper quintile of daily energetic intake of UPF presented an increased risk of the outcome (RR: 1 center dot 35; 95 \% CI 1 center dot 01, 1 center dot 81). Conclusions: In this prospective cohort of Brazilian middle-aged adult university graduates, the highest consumptions of U/MPF\&CP and UPF were associated with, respectively, reduced and increased risk of hypertension. Additional longitudinal studies are needed to confirm our results.},
  copyright = {The Author(s), 2020. Published by Cambridge University Press on behalf of The Nutrition Society},
  langid = {english},
  keywords = {Adults,Antihypertensives,Blood pressure,Cardiology,Colleges & universities,Confidence intervals,Disease,Environmental & Occupational Health,Family income,Food,Food consumption,Food processing,Fruits,Hypertension,Life Sciences & Biomedicine,Lifestyles,Longitudinal studies,Medical diagnosis,Nutrition & Dietetics,Nutritional epidemiology,Oils & fats,Processed foods,Public,Questionnaires,Regression analysis,Regression models,Research paper,Risk assessment,Risk management,Risk reduction,Robustness (mathematics),Science & Technology,Sociodemographics,Statistical analysis,Ultra-processed food,Womens health}
