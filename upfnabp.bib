  title = {Intersalt: An International Study of Electrolyte Excretion and Blood Pressure. {{Results}} for 24 Hour Urinary Sodium and Potassium Excretion. {{Intersalt Cooperative Research Group}}.},
  shorttitle = {Intersalt},
  year = {1988},
