@ARTICLE{Bibbins-Domingo2010590,
author={Bibbins-Domingo, K. and Chertow, G.M. and Coxson, P.G. and Moran, A. and Lightwood, J.M. and Pletcher, M.J. and Goldman, L.},
title={Projected effect of dietary salt reductions on future cardiovascular disease},
